<binding xmlns:ns2="http://dataone.org/service/types/ObjectList/0.5" xmlns:common="http://dataone.org/service/types/common/0.5" trim-whitespace="true">
  <namespace uri="http://dataone.org/service/types/ObjectList/0.5" prefix="d1"/>
  <mapping abstract="true" type-name="ns2:ObjectInfo" class="org.dataone.service.types.ObjectInfo">
    <structure map-as="common:Identifier" get-method="getIdentifier" set-method="setIdentifier" name="identifier"/>
    <value style="element" name="objectFormat" get-method="getObjectFormat" set-method="setObjectFormat" format="common:ObjectFormat"/>
    <structure map-as="common:Checksum" get-method="getChecksum" set-method="setChecksum" name="checksum"/>
    <value style="element" name="dateSysMetadataModified" get-method="getDateSysMetadataModified" set-method="setDateSysMetadataModified"/>
    <value style="element" name="size" get-method="getSize" set-method="setSize"/>
  </mapping>
  <mapping abstract="true" type-name="ns2:ObjectList" class="org.dataone.service.types.ObjectList">
    <structure map-as="common:Slice"/>
    <collection get-method="getObjectInfoList" set-method="setObjectInfoList" create-type="java.util.ArrayList">
      <structure map-as="ns2:ObjectInfo" name="objectInfo"/>
    </collection>
  </mapping>
  <mapping class="org.dataone.service.types.ObjectList" name="objectList" ns="http://dataone.org/service/types/ObjectList/0.5">
    <structure map-as="ns2:ObjectList"/>
  </mapping>
</binding>