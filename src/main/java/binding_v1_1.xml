<binding xmlns:d1_v1.1="http://ns.dataone.org/service/types/v1.1" trim-whitespace="true">
  <namespace uri="http://ns.dataone.org/service/types/v1.1" prefix="d1_v1.1"/>
  <mapping abstract="true" type-name="d1_v1.1:QueryEngineDescription" class="org.dataone.service.types.v1_1.QueryEngineDescription">
    <value style="element" name="queryEngineVersion" get-method="getQueryEngineVersion" set-method="setQueryEngineVersion"/>
    <value style="element" name="querySchemaVersion" get-method="getQuerySchemaVersion" set-method="setQuerySchemaVersion" usage="optional"/>
    <value style="element" name="name" get-method="getName" set-method="setName"/>
    <collection get-method="getAdditionalInfoList" set-method="setAdditionalInfoList" usage="optional" create-type="java.util.ArrayList">
      <value style="element" name="additionalInfo" type="java.lang.String"/>
    </collection>
    <collection get-method="getQueryFieldList" set-method="setQueryFieldList" usage="optional" create-type="java.util.ArrayList">
      <structure map-as="d1_v1.1:QueryField" name="queryField"/>
    </collection>
  </mapping>
  <mapping class="org.dataone.service.types.v1_1.QueryEngineDescription" name="queryEngineDescription" ns="http://ns.dataone.org/service/types/v1.1">
    <structure map-as="d1_v1.1:QueryEngineDescription"/>
  </mapping>
  <mapping abstract="true" type-name="d1_v1.1:QueryEngineList" class="org.dataone.service.types.v1_1.QueryEngineList">
    <collection get-method="getQueryEngineList" set-method="setQueryEngineList" usage="optional" create-type="java.util.ArrayList">
      <value style="element" name="queryEngine" type="java.lang.String"/>
    </collection>
  </mapping>
  <mapping class="org.dataone.service.types.v1_1.QueryEngineList" name="queryEngineList" ns="http://ns.dataone.org/service/types/v1.1">
    <structure map-as="d1_v1.1:QueryEngineList"/>
  </mapping>
  <mapping abstract="true" type-name="d1_v1.1:QueryField" class="org.dataone.service.types.v1_1.QueryField">
    <value style="element" name="name" get-method="getName" set-method="setName"/>
    <collection get-method="getDescriptionList" set-method="setDescriptionList" usage="optional" create-type="java.util.ArrayList">
      <value style="element" name="description" type="java.lang.String"/>
    </collection>
    <value style="element" name="type" get-method="getType" set-method="setType"/>
    <value style="element" name="searchable" get-method="isSearchable" set-method="setSearchable"/>
    <value style="element" name="returnable" get-method="isReturnable" set-method="setReturnable"/>
    <value style="element" name="sortable" get-method="isSortable" set-method="setSortable"/>
    <value style="element" name="multivalued" get-method="getMultivalued" set-method="setMultivalued" usage="optional"/>
  </mapping>
</binding>