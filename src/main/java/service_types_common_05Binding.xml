<binding xmlns:common="http://dataone.org/service/types/common/0.5" trim-whitespace="true">
  <mapping abstract="true" type-name="common:Identifier" class="org.dataone.service.types.Identifier">
    <value style="text" get-method="getValue" set-method="setValue"/>
  </mapping>
  <mapping abstract="true" type-name="common:NodeReference" class="org.dataone.service.types.NodeReference">
    <value style="text" get-method="getValue" set-method="setValue"/>
  </mapping>
  <mapping abstract="true" type-name="common:Principal" class="org.dataone.service.types.Principal">
    <value style="text" get-method="getValue" set-method="setValue"/>
  </mapping>
  <mapping abstract="true" type-name="common:ComponentVersion" class="org.dataone.service.types.ComponentVersion">
    <value style="text" get-method="getValue" set-method="setValue"/>
  </mapping>
  <mapping abstract="true" type-name="common:Checksum" class="org.dataone.service.types.Checksum">
    <value style="text" get-method="getValue" set-method="setValue"/>
    <value style="attribute" name="algorithm" get-method="getAlgorithm" set-method="setAlgorithm" format="common:ChecksumAlgorithm"/>
  </mapping>
  <mapping abstract="true" type-name="common:Slice" class="org.dataone.service.types.Slice">
    <value style="attribute" name="count" get-method="getCount" set-method="setCount"/>
    <value style="attribute" name="start" get-method="getStart" set-method="setStart"/>
    <value style="attribute" name="total" get-method="getTotal" set-method="setTotal"/>
  </mapping>
</binding>