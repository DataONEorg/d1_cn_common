<binding xmlns:d1="http://dataone.org/service/types/ObjectList/0.1" xmlns:common="http://dataone.org/service/types/common/0.1" trim-whitespace="true">
  <namespace uri="http://dataone.org/service/types/ObjectList/0.1" prefix="d1"/>
  <mapping abstract="true" type-name="d1:ObjectInfo" class="org.dataone.service.types.ObjectInfo">
    <structure map-as="common:Identifier" get-method="getIdentifier" set-method="setIdentifier" name="identifier"/>
    <value style="element" name="objectFormat" get-method="getObjectFormat" set-method="setObjectFormat" format="common:ObjectFormat"/>
    <structure map-as="common:Checksum" get-method="getChecksum" set-method="setChecksum" name="checksum"/>
    <value style="element" name="dateSysMetadataModified" get-method="getDateSysMetadataModified" set-method="setDateSysMetadataModified"/>
    <value style="element" name="size" get-method="getSize" set-method="setSize"/>
  </mapping>
  <mapping abstract="true" type-name="d1:ObjectList" class="org.dataone.service.types.ObjectList">
    <collection get-method="getObjectInfoList" set-method="setObjectInfoList" create-type="java.util.ArrayList">
      <structure map-as="d1:ObjectInfo" name="objectInfo"/>
    </collection>
    <value style="attribute" name="count" get-method="getCount" set-method="setCount"/>
    <value style="attribute" name="start" get-method="getStart" set-method="setStart"/>
    <value style="attribute" name="total" get-method="getTotal" set-method="setTotal"/>
  </mapping>
  <mapping class="org.dataone.service.types.ObjectList" name="objectList" ns="http://dataone.org/service/types/ObjectList/0.1">
    <structure map-as="d1:ObjectList"/>
  </mapping>
</binding>