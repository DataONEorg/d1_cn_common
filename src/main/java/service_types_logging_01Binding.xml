<binding xmlns:ns2="http://dataone.org/service/types/logging/0.1" xmlns:common="http://dataone.org/service/types/common/0.1" trim-whitespace="true">
  <namespace uri="http://dataone.org/service/types/logging/0.1" prefix="d1"/>
  <mapping abstract="true" type-name="ns2:LogEntry" class="org.dataone.service.types.LogEntry">
    <structure map-as="common:Identifier" get-method="getEntryId" set-method="setEntryId" name="entryId"/>
    <structure map-as="common:Identifier" get-method="getIdentifier" set-method="setIdentifier" name="identifier"/>
    <value style="element" name="ipAddress" get-method="getIpAddress" set-method="setIpAddress"/>
    <value style="element" name="userAgent" get-method="getUserAgent" set-method="setUserAgent"/>
    <structure map-as="common:Principal" get-method="getPrincipal" set-method="setPrincipal" name="principal"/>
    <value style="element" name="event" get-method="getEvent" set-method="setEvent" format="ns2:Event"/>
    <value style="element" name="dateLogged" get-method="getDateLogged" set-method="setDateLogged"/>
    <structure map-as="common:NodeReference" get-method="getMemberNode" set-method="setMemberNode" name="memberNode"/>
  </mapping>
  <mapping class="org.dataone.service.types.Log" name="log" ns="http://dataone.org/service/types/logging/0.1">
    <collection get-method="getLogEntryList" set-method="setLogEntryList" create-type="java.util.ArrayList">
      <structure map-as="ns2:LogEntry" name="logEntry"/>
    </collection>
  </mapping>
</binding>